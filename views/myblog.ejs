<%  posts.forEach(function(post){ %>

    <h1 class="all-blog-h1" data-aos="slide-up"><%=post.title%></h1>
    <p class="all-blog-para" data-aos="slide-up"> <%=post.content%></p>

    <form class="post-db-main" action="/deleteb" method="post">
        <div class="form-group">
        </div> 
        <button class="btn btn-block btn-dark btn-lg click-del" type="submit" name="theitem" value="<%=post._id%>"> <a class="card-link">CLICK HERE TO DELETE THIS POST</a> </button>
      </form>
    <hr class="blog-hr">
   

    <% }) %>
